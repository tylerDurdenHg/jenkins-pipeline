pipeline {
    agent any
    tools {
        maven 'Maven'
    }
    environment {
        MY_HI= "HELLO"
    }
    
    stages {
        stage('Preparation') {
        	steps {
	       		sh 'echo Hi its me'
           		sh 'echo $MY_HI'
	        }
        }
        stage('Compile') {
        	steps {
	            sh 'mvn compile'
	        }
        }
    }
}